<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="195.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="LoginController">
   <children>
      <TextField fx:id="userNameField" layoutX="114.0" layoutY="50.0" prefHeight="26.0" prefWidth="281.0"/>
      <TextField fx:id="passwordField" layoutX="114.0" layoutY="92.0" prefHeight="26.0" prefWidth="282.0"/>
      <Label layoutX="35.0" layoutY="54.0" text="Username"/>
      <Label layoutX="35.0" layoutY="96.0" text="Password"/>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="125.0" layoutY="14.0" prefHeight="18.0"
             prefWidth="152.0" text="Book Management">
         <font>
            <Font size="16.0"/>
         </font>
      </Label>
      <Button fx:id="login" layoutX="178.0" layoutY="145.0" mnemonicParsing="false" onAction="#onLoginClicked"
              prefHeight="26.0" prefWidth="66.0" text="Login"/>
      <CheckBox fx:id="rememberMe" layoutX="286.0" layoutY="149.0" mnemonicParsing="false" prefHeight="18.0"
                prefWidth="152.0" text="Remember Me"/>
   </children>
</AnchorPane>
